{
  "compilerOptions": {
    "newLine": "LF",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noEmit": true,
    "jsx": "preserve"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
